  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "cy:mydailydigest.me": "nx e2e mydailydigest.me-e2e",
    "cy:admin.mydailydigest.me": "nx e2e admin.mydailydigest.me-e2e",
    "cy:all": "pnpm run cy:admin.mydailydigest.me && pnpm run cy:mydailydigest.me",
    "copy:mydailydigest.me:assets": "rsync -a --ignore-existing common/mydailydigest/public/ apps/mydailydigest.me/public/",
    "copy:admin.mydailydigest.me:assets": "rsync -a --ignore-existing common/mydailydigest/public/ apps/admin.mydailydigest.me/public/",
    "copy:mydailydigest:global:assets": "pnpm run copy:mydailydigest.me:assets && pnpm run copy:admin.mydailydigest.me:assets",
    "lint:core": "nx lint core",
    "lint:angular": "nx lint angular",
    "lint:mydailydigest": "nx lint mydailydigest",
    "lint:mydailydigest.me": "nx lint mydailydigest.me",
    "lint:admin.mydailydigest.me": "nx lint admin.mydailydigest.me",
    "lint:all": "pnpm run lint:core && pnpm run lint:angular && pnpm run lint:ui && pnpm run lint:common && pnpm run lint:mydailydigest.me && pnpm run lint:admin.mydailydigest.me",
    "serve:mydailydigest.me": "pnpm run copy:mydailydigest:global:assets && nx run mydailydigest.me:serve:development --port=4400",
    "serve:admin.mydailydigest.me": "pnpm run copy:admin.mydailydigest.me:assets && nx run admin.mydailydigest.me:serve:development --port=4300",
    "serve:mydailydigest": "pnpm run copy:mydailydigest:global:assets && pnpm run serve:admin.mydailydigest.me && pnpm run serve:mydailydigest.me",
    "build:admin.mydailydigest.me": "nx build admin.mydailydigest.me",
    "build:mydailydigest.me": "nx build mydailydigest.me",
    "build:mydailydigest": "pnpm run build:mydailydigest.me && pnpm run build:admin.mydailydigest.me",
    "build:admin.mydailydigest.me:prod": "nx build admin.mydailydigest.me --configuration=production",
    "build:mydailydigest.me:prod": "nx build mydailydigest.me --configuration=production",
    "build:mydailydigest:prod": "pnpm run build:mydailydigest.me:prod && pnpm run build:admin.mydailydigest.me:prod",
    "build:admin.mydailydigest.me:staging": "nx build admin.mydailydigest.me --configuration=staging",
    "build:mydailydigest.me:staging": "nx build mydailydigest.me --configuration=staging",
    "build:mydailydigest:staging": "pnpm run build:mydailydigest.me:staging && pnpm run build:admin.mydailydigest.me:staging",
    "test:admin.mydailydigest.me": "nx test admin.mydailydigest.me",
    "test:mydailydigest.me": "nx test mydailydigest.me",
    "test:core": "nx test core",
    "test:mydailydigest": "nx test mydailydigest",
    "test:angular": "nx test angular",
    "test:all": "pnpm run test:core && pnpm run test:angular && pnpm run test:ui && pnpm run test:common && pnpm run test:mydailydigest.me && pnpm run test:admin.mydailydigest.me"
  },
  "private": true,
